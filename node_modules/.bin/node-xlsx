#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tiandaoyi/docker-project/i18n-cli/node_modules/.pnpm/node-xlsx@0.21.0/node_modules/node-xlsx/lib/bin/node_modules:/Users/tiandaoyi/docker-project/i18n-cli/node_modules/.pnpm/node-xlsx@0.21.0/node_modules/node-xlsx/lib/node_modules:/Users/tiandaoyi/docker-project/i18n-cli/node_modules/.pnpm/node-xlsx@0.21.0/node_modules/node-xlsx/node_modules:/Users/tiandaoyi/docker-project/i18n-cli/node_modules/.pnpm/node-xlsx@0.21.0/node_modules:/Users/tiandaoyi/docker-project/i18n-cli/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tiandaoyi/docker-project/i18n-cli/node_modules/.pnpm/node-xlsx@0.21.0/node_modules/node-xlsx/lib/bin/node_modules:/Users/tiandaoyi/docker-project/i18n-cli/node_modules/.pnpm/node-xlsx@0.21.0/node_modules/node-xlsx/lib/node_modules:/Users/tiandaoyi/docker-project/i18n-cli/node_modules/.pnpm/node-xlsx@0.21.0/node_modules/node-xlsx/node_modules:/Users/tiandaoyi/docker-project/i18n-cli/node_modules/.pnpm/node-xlsx@0.21.0/node_modules:/Users/tiandaoyi/docker-project/i18n-cli/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/node-xlsx@0.21.0/node_modules/node-xlsx/lib/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/node-xlsx@0.21.0/node_modules/node-xlsx/lib/bin/cli.js" "$@"
fi
